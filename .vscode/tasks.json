{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe 生成活动文件",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/nologo",
                "/I\"${fileDirname}\\include\"",
                "/Fe${fileDirname}\\${fileBasenameNoExtension}.exe",
                "${file}",
                "/link",
                "/LIBPATH:\"${fileDirname}\\lib\"",
                "onnxruntime.lib",
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        },
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe 生成 DLL",
            "command": "cl.exe",
            "args": [
                "/Zi", // 调试信息
                "/EHsc", // 异常处理
                "/nologo", // 不显示版权信息
                "/I\"${fileDirname}\\include\"",
                "/LD", // 生成DLL
                "/MD", // 使用多线程DLL运行时
                "/Fe${fileDirname}\\${fileBasenameNoExtension}.dll", // 输出DLL文件
                "${file}", // 源文件
                "/link", // 开始链接参数
                "/LIBPATH:\"${fileDirname}\\lib\"",
                "onnxruntime.lib" // 链接ONNX Runtime库
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "生成DLL的调试器任务"
        }
    ],
    "version": "2.0.0"
}